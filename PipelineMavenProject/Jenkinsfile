pipeline 
{ 
    agent any 
     tools { 
        maven 'maven-3.6' 
        jdk 'java-1.8' 
    }
    stages 
	{ 
	stage ('Initialize Env')
	{
            steps 
	     {
                bat '''
                    echo "PATH =" ${PATH}
                    echo "M2_HOME =" ${MAVEN_HOME}
                ''' 
             }
        }	
	
        stage('Testing echo') 
	 {
            steps 
        	{  
	        echo 'Hello machiii'
               }
        }
		
    }
}
