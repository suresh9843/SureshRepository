pipeline 
{ 
    agent any 
     tools { 
        maven 'maven-3.6' 
        jdk 'java-1.8' 
    }
    stages 
	{ 
	stage ('Initialize Env')
	{
            steps 
	     {
		bat 'set'
                bat 'echo "PATH = ${PATH}"'
                bat 'echo "M2_HOME = ${MAVEN_HOME}"' 
		bat '''
                     echo 'Multiline'
                     echo 'Example'
                    '''
             }
        }	
	
        stage('Testing echo') 
	 {
            steps 
        	{  
	        echo 'Hello machiii'
               }
        }
		
    }
}
